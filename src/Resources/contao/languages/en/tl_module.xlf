  <file datatype="php" original="src/Resources/contao/languages/en/tl_module.php" source-language="en">
    <body>
      <trans-unit id="tl_module.google_maps_legend">
        <source>Google-Map Settings</source>
      </trans-unit>

      <trans-unit id="tl_module.googleInitialLat.0">
        <source>Latitude</source>
      </trans-unit>
      <trans-unit id="tl_module.googleInitialLat.1">
        <source>Defines the initial area after the card has been loaded.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleInitialLng.0">
        <source>Longitude</source>
      </trans-unit>
      <trans-unit id="tl_module.googleInitialLng.1">
        <source>Defines the initial area after the card has been loaded.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleInitialZoom.0">
        <source>Zoom level</source>
      </trans-unit>
      <trans-unit id="tl_module.googleInitialZoom.1">
        <source>Defines the initial zoom level. This value is ignored when using 'googleUseBounce'.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleMinZoom.0">
        <source>min. zoom level</source>
      </trans-unit>
      <trans-unit id="tl_module.googleMinZoom.1">
        <source>Defines the minimum zoom level, that the user can reach by scrolling or pressing '+'/'-'.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleMaxZoom.0">
        <source>max. zoom level</source>
      </trans-unit>
      <trans-unit id="tl_module.googleMaxZoom.1">
        <source>Defines the maximum zoom level, that the user can reach by scrolling or pressing '+'/'-'.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleType.0">
        <source>Type of map view</source>
      </trans-unit>
      <trans-unit id="tl_module.googleType.1">
        <source>Defines the default representation of the map.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleGestureHandling.0">
        <source>Gesture Handling</source>
      </trans-unit>
      <trans-unit id="tl_module.googleGestureHandling.1">
        <source>Defines the behavior of the interaction with the map.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleUseCluster.0">
        <source>Use Clustering</source>
      </trans-unit>
      <trans-unit id="tl_module.googleUseCluster.1">
        <source>Combines close located markers. The appearance can be specified in the Real Estate Management settings.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleUseSpiderfier.0">
        <source>Spiderfier for overlapping markers</source>
      </trans-unit>
      <trans-unit id="tl_module.googleUseSpiderfier.1">
        <source>Combines closely spaced markers and spreads them out when being clicked.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleUseBounce.0">
        <source>Use Bounce</source>
      </trans-unit>
      <trans-unit id="tl_module.googleUseBounce.1">
        <source>Changes the visible area of the map (zoom, longitude and latitude) to display all markers on the map.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleInteractive.0">
        <source>Allow interaction</source>
      </trans-unit>
      <trans-unit id="tl_module.googleInteractive.1">
        <source>Allows moving around whilst holding the mouse button and zooming by double clicking.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleFilterAddSorting.0">
        <source>Add location sorting</source>
      </trans-unit>
      <trans-unit id="tl_module.googleFilterAddSorting.1">
        <source>Adds a sorting option based on a location.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleFilterLat.0">
        <source>Longitude</source>
      </trans-unit>
      <trans-unit id="tl_module.googleFilterLat.1">
        <source>Here you need to enter the longitude value for the location sorting.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleFilterLng.0">
        <source>Latitude</source>
      </trans-unit>
      <trans-unit id="tl_module.googleFilterLng.1">
        <source>Here you need to enter the latitude value for the location sorting.</source>
      </trans-unit>

      <trans-unit id="tl_module.googleControls.0">
        <source>Zoom control</source>
      </trans-unit>
      <trans-unit id="tl_module.googleControls.1">
        <source>Display zoom buttons ('+'/'-').</source>
      </trans-unit>
      <trans-unit id="tl_module.googleFullscreen.0">
        <source>Allow full screen mode</source>
      </trans-unit>
      <trans-unit id="tl_module.googleFullscreen.1">
        <source>Display a button for full screen mode.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleStreetview.0">
        <source>Streetview</source>
      </trans-unit>
      <trans-unit id="tl_module.googleStreetview.1">
        <source>Display a button for streetview-mode.</source>
      </trans-unit>
      <trans-unit id="tl_module.googleMapTypeControl.0">
        <source>Customize map view</source>
      </trans-unit>
      <trans-unit id="tl_module.googleMapTypeControl.1">
        <source>Displays the available map view. Allows users to change how the map is being displayed.</source>
      </trans-unit>

      <trans-unit id="tl_module.location.0">
        <source>Location</source>
      </trans-unit>

      <trans-unit id="tl_module.googleMapPopupTemplate.0">
        <source>Google-Maps Popup-Template</source>
      </trans-unit>
      <trans-unit id="tl_module.googleMapPopupTemplate.1">
        <source>Here you can overwrite the default template.</source>
      </trans-unit>

    </body>
  </file>
